set title "Gold over time for each algorithm for different grid sizes"
set xlabel "Size ($S$)"
set ylabel "Gold over time ($\\sigma$)"
set autoscale
set xtic auto
set ytic auto

set term epslatex
set output "gold_sizes.eps"
plot "gold_size.dat" using 1:2 title 'Na\"ive' with lines, \
	"gold_size.dat" using 1:3 title 'Desert Ant' with lines, \
	"gold_size.dat" using 1:4 title 'Honey Bee' with lines
	
